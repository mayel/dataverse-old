{% extends "layout.html.twig" %}

{% block content %}

<div class="row justify-content-md-center" >
  <div class="col col-md-6">
  
<h1>{{ title|e }}</h1>

<a href="/build/questionnaire?new"  class="btn btn-info float-right">New Questionnaire</a>

<ul class="clearfix">
    {% for v in data %}
        <li>
        <a href="/build/questionnaire?id={{ v.id|e }}">
        	{{ v.questionnaire_title|e }}
        </a>
        
        </li>
    {% endfor %}
</ul>

</div>
</div>

{% endblock %}
